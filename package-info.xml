<?xml version="1.0"?>
<!DOCTYPE package-info SYSTEM "http://www.hippoedit.com/xml/package-info">
<package-info xmlns="http://www.hippoedit.com/xml/package-info" xmlns:smf="http://www.hippoedit.com/">
	<id>D66272B6-0571-4CB7-AA7E-74251423FEC8</id>
	<name>OpenURL</name>
	<version>1.1</version>
	<description>Open content of remote URL in text editor</description>
	<author>HippoEDIT.com</author>
	<email>supportbox@hippoedit.com</email>
	<homepage>http://wiki.hippoedit.com/plugins/open-url</homepage>
	<memo></memo>
	<install>
		<readme type="inline">This will install OpenURL plugin for HippoEDIT.</readme>
		<require-file name="openurl.hejs" destination="%{plugins}%/openurl.hejs" />
		<install-plugin path="%{plugins}%/openurl.hejs"/>
	</install>
	<uninstall>
		<readme type="inline">This will uninstall OpenURL plugin for HippoEDIT.</readme>
		<uninstall-plugin id="D66272B6-0571-4CB7-AA7E-74251423FEC8"/>
		<remove-file name="%{plugins}%/openurl.hejs" />
	</uninstall>
</package-info>
